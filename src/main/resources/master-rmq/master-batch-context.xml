	</bean>
	<bean id="chunkHandler"
		class="org.springframework.batch.integration.chunk.ChunkMasterHandlerFactoryBean">
		<property name="chunkWriter" ref="chunkWriter" />
		<property name="step" ref="step" />
	</bean>

	<bean id="masterMessagingGateway" class="org.springframework.batch.message.ChunkMessageMasterGateway" init-method="onInit" destroy-method="onClose">
